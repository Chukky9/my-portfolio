{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "My portfolio",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve",
    "start": "node server.js",
    "build": "webpack --mode production",
    "test": "jest --watch --env=jsdom"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "\\.(js|jsx)?$": "<rootDir>/node_modules/babel-jest",
      ".+\\.(css|scss|png|jpg|svg)$": "jest-transform-stub"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/public/"
    ],
    "setupFilesAfterEnv": ["<rootDir>/src/setupTests.js"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chukky9/my-portfolio.git"
  },
  "author": "Alozie Chukwuemeka",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Chukky9/my-portfolio/issues"
  },
  "homepage": "https://github.com/Chukky9/my-portfolio#readme",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.4.7",
    "jest-transform-stub": "^2.0.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "bootstrap": "^5.0.1",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
